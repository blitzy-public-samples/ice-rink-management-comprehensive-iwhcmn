NEXT_PUBLIC_API_BASE_URL=http://localhost:3000/api
NEXT_PUBLIC_API_VERSION=v1
NEXT_PUBLIC_API_TIMEOUT=5000

# Application Information
NEXT_PUBLIC_APP_NAME="Ice Rink Management System - Development"
NEXT_PUBLIC_APP_VERSION=1.0.0

# External Services
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_DEVELOPMENT_GOOGLE_MAPS_API_KEY
NEXT_PUBLIC_STRIPE_PUBLIC_KEY=YOUR_DEVELOPMENT_STRIPE_PUBLIC_KEY
NEXT_PUBLIC_FIREBASE_CONFIG={"apiKey":"YOUR_FIREBASE_API_KEY","authDomain":"your-dev-project.firebaseapp.com","projectId":"your-dev-project","storageBucket":"your-dev-project.appspot.com","messagingSenderId":"YOUR_MESSAGING_SENDER_ID","appId":"YOUR_APP_ID","measurementId":"YOUR_MEASUREMENT_ID"}
NEXT_PUBLIC_SENTRY_DSN=YOUR_DEVELOPMENT_SENTRY_DSN

# Feature Flags
NEXT_PUBLIC_FEATURE_FLAG_EQUIPMENT_RENTAL=true
NEXT_PUBLIC_FEATURE_FLAG_ADVANCED_ANALYTICS=false

# Development Settings
NEXT_PUBLIC_DEBUG_MODE=true

# Human Tasks:
# - Verify and update all environment variables with correct development values
# - Ensure that sensitive information is not committed to version control
# - Regularly review and update these variables to match any changes in the development environment